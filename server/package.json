  "description": "",
  "main": "index.js",
  "devDependencies": {
    "@types/express": "^4.17.12",
    "@types/jsonwebtoken": "^8.5.3",
    "@types/mock-fs": "^4.13.0",
    "@types/node": "^15.12.5",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.1",
